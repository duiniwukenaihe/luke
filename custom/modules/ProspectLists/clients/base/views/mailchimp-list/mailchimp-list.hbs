<link rel="stylesheet" type="text/css" href="custom/themes/mailchimp-styles.css">
{{#if listStats.name}}
    <div class="list-info">
        <ul class="favorit-list">
            <li><i class="fa {{listStats.star_one_class}}"></i></li>
            <li><i class="fa {{listStats.star_two_class}}"></i></li>
            <li><i class="fa {{listStats.star_three_class}}"></i></li>
            <li><i class="fa {{listStats.star_four_class}}"></i></li>
            <li><i class="fa {{listStats.star_five_class}}"></i></li>
        </ul>
        <h5>{{str "LBL_MAILCHIMP_LIST_COLON"}} <span>{{listStats.name}}</span></h5>
    </div>
    <div class="list-stats">
        <ul>
            <li>
                <h3>
                    {{listStats.subscribers}}
                    <span>{{str "LBL_MAILCHIMP_SUBSCRIBERS"}}</span>
                </h3>
            </li>
            <li>
                <h3>
                    {{listStats.open_rate}}%
                    <span>{{str "LBL_MAILCHIMP_OPEN_RATE"}}</span>
                </h3>
            </li>
            <li>
                <h3>
                    {{listStats.click_rate}}%
                    <span>{{str "LBL_MAILCHIMP_CLICK_RATE"}}</span>
                </h3>
            </li>
        </ul>
    </div>
{{/if}}
